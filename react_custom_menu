import React, { useState, useEffect, useRef } from 'react';
import { Space, Breadcrumb, Dropdown, Button, Menu, Table } from 'antd';
import { MoreOutlined, PlusOutlined } from '@ant-design/icons';

var last_three_dot_id = '';

function Company_OverView() {
 // listing table view
  useEffect(() => {
    document.addEventListener('click', handleClickOutside, true);
    var for_scroll_detect = document.getElementById('sr_btm_content');
    for_scroll_detect.addEventListener('scroll', () => {
      var find_if_dropdown_div_exists = document.getElementById(
        'fixed_right_bar_overview'
      );
      if (find_if_dropdown_div_exists) {
        if (last_three_dot_id) {
          var get_offset_of_three_offset =
            document?.getElementById(last_three_dot_id);
          var offset = get_offset_of_three_offset?.getBoundingClientRect();
          // alert(offset.top);
          var bodyRect = document?.body?.getBoundingClientRect();
          offset = offset?.top - bodyRect?.top - 100;
          find_if_dropdown_div_exists.style.top = offset + 'px';
        }
      }
    });
  });
  }


const company_action_function = (e, id) => {
    var check_s = document.getElementById('fixed_right_bar_overview');
    last_three_dot_id = 'three_dots_overview' + id;

    var bodyRect = document.body.getBoundingClientRect(),
      elemRect = e.getBoundingClientRect(),
      offset = elemRect.top - 60;
    var right = 25;
    check_s.setAttribute(
      'style',
      'position: fixed;height: 300px !important;width: 200px !important;z-index: 8 !important; right:90px !important; top:' +
        offset +
        'px'
    );
    // document.body.appendChild(g);
    console.log('Element is ' + offset + ' vertical pixels from <body>');
  };

 const refOne = useRef(null);
  const handleClickOutside = (e) => {
    var check = refOne.current;
    var check_option = localStorage.getItem('deleteoption');
    if (check_option == 'true') {
      if (check_option == 'true') {
        CheckDelete = true;
      }
    }

    if (check == null || !refOne.current.contains(e.target)) {
      if (
        e.target.className != 'parent_list_action_timetac' ||
        e.target.id == 'move_to_inner' ||
        e.target.id == 'ClickState' ||
        e.target.id == 'custom_flag_vals' ||
        e.target.nodeName == 'svg' ||
        e.target.id == 'Deletebtn'
      ) {
      } else {
        if (e.target.className != 'parent_list_action_timetac') {
          var check_s = document.getElementById('fixed_right_bar_overview');
          if (check_s) {
            check_s.style.display = 'none';
          }
        }

        //props.DeletePopUpFun(false);
        //  props.setMoveTo_Fun(null);
      }
    } else {
      if (!CheckDelete) {
        console.log('inside');
        CheckDelete = false;
      }
    }

            rowKey={'id'}
          />
        </section>
      )}



     <div
        ref={refOne}
        className="fixed_right_bar_overview sr_my_listing"
        id="fixed_right_bar_overview"
        style={{ display: companyActionOption != null ? 'block' : 'none' }}
      >
        <div id="wrapper">
          <div className="menu__wrap r-0" id="ClickState">
            <ul className="submenu m-0" id="ClickState">
              <li id="ClickState" className="sidebar-link">
                <a id="ClickState" className="link">
                  <i id="ClickState" className="icon-Edit"></i>
                  Edit
                </a>
              </li>

              <li id="ClickState" className="sidebar-link">
                <a id="ClickState" className="link">
                  <i id="ClickState" className="icon-Delete"></i>
                  Delete
                </a>
              </li>
            </ul>
          </div>
        </div>
      </div>
